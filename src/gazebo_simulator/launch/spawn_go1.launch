<?xml version="1.0"?>

<launch>

  <!-- <arg name="multimaster" default="false"/> -->

  <arg name="robot_namespace" default="/"/>

  <arg name="x" default="0.07"/>
  <arg name="y" default="13.9"/>
  <arg name="z" default="0.5"/>
  <arg name="yaw" default="-1.6"/>

  <group ns="$(arg robot_namespace)">

  <param name="robot_description" command="$(find xacro)/xacro '$(find gazebo_simulator)/urdf/robot.xacro' "/>

  <!-- Spawn robot in gazebo -->
    <node name="spawn_go1_model" pkg="gazebo_ros" type="spawn_model"
        args="-x $(arg x)
              -y $(arg y)
              -z $(arg z)
              -Y $(arg yaw)
              -unpause
              -urdf
              -param robot_description
              -model go1
              -robot_namespace $(arg robot_namespace)" />

  </group>

</launch>
